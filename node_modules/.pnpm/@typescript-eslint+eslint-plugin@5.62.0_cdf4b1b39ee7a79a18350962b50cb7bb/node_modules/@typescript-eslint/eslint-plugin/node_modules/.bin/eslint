#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/eslint@8.46.0/node_modules/eslint/bin/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/eslint@8.46.0/node_modules/eslint/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/eslint@8.46.0/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_cdf4b1b39ee7a79a18350962b50cb7bb/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_cdf4b1b39ee7a79a18350962b50cb7bb/node_modules/@typescript-eslint/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_cdf4b1b39ee7a79a18350962b50cb7bb/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/eslint@8.46.0/node_modules/eslint/bin/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/eslint@8.46.0/node_modules/eslint/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/eslint@8.46.0/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_cdf4b1b39ee7a79a18350962b50cb7bb/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_cdf4b1b39ee7a79a18350962b50cb7bb/node_modules/@typescript-eslint/node_modules:/home/workspace/reactjs/coding-practices/transactionmanagementapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_cdf4b1b39ee7a79a18350962b50cb7bb/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.46.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.46.0/node_modules/eslint/bin/eslint.js" "$@"
fi
